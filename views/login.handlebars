<div class="row justify-content-around">
  <div class="col-4">
    <!-- sign in -->
    <div class="sign-in">
      <h3 class="title">Sign-In</h3>
      <label for="email">Email:</label>
      <input
        class="input"
        type="email"
        id="loginemail"
        name="email"
        size="35"
      /><br /><br />
      <label for="pwd">Password:</label>
      <input
        class="input"
        type="password"
        id="loginpwd"
        name="pwd"
        minlength="8"
        size="35"
      /><br /><br />
      <input id="signinbttn" class="buttons-two" type="submit" />
      <hr />
      <div class="g-signin2" data-onsuccess="onSignIn" data-theme="dark"></div>
    </div>
  </div>
  <div class="col-4">
    <!-- sign up -->
    <div class="sign-up">
      <h3 class="title">Sign-Up</h3>
      <label for="fname">First name: </label>
      <input
        class="input-two"
        type="text"
        id="fnamesignup"
        name="fname"
        size="35"
      /><br /><br />
      <label for="lname">Last name: </label>
      <input
        class="input-two"
        type="text"
        id="lnamesignup"
        name="lname"
        size="35"
      /><br /><br />
      <label for="email">Email:</label>
      <input
        class="input-two"
        type="email"
        id="emailsignup"
        name="email"
        size="35"
      /><br /><br />
      <label for="lname">Username: </label>
      <input
        class="input-two"
        type="text"
        id="userinfoname"
        name="username"
        size="35"
      /><br /><br />
      <!-- <label for="phone">Enter a phone number:</label><br /><br />
        <input class="input"
          type="tel"
          id="phone"
          name="phone"
          placeholder="123-456-789"
          pattern="[0-9]{3}-[0-9]{3}-[0-9]{4}"
          required
        /><br /><br />
        <small>Format: 123-456-789</small><br /><br /> -->
      <label for="pwd">Password:</label>
      <input
        class="input-two"
        type="password"
        id="passwordsignup"
        name="pwd"
        minlength="8"
        size="35"
      /><br /><br />
      <input id="signupbttn" class="buttons" type="submit" />
      <hr />
      <div class="g-signin2" data-onsuccess="onSignIn" data-theme="dark"></div>
    </div>
  </div>
</div>
